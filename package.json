{
  "name": "dApp-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "drizzle": "^1.2.2",
    "drizzle-react": "^1.1.1",
    "drizzle-react-components": "^1.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "app-start": "react-scripts-ts start",
    "app-build": "react-scripts-ts build",
    "app-serve-build": "./node_modules/.bin/serve -s build",
    "app-test": "react-scripts-ts test --env=jsdom",
    "eth-start": "npm run eth-start-xtrerm:genache && sleep 10s && npm run eth-deploy",
    "eth-start:genache": "./node_modules/.bin/ganache-cli -d --gasLimit 7000000000000 --acctKeys ./ganache-accounts.json",
    "eth-start-xtrerm:genache": "xterm -e './node_modules/.bin/ganache-cli -d --gasLimit 7000000000000 --acctKeys ./ganache-accounts.json' &",
    "eth-deploy": "./node_modules/.bin/truffle migrate --compile-all --reset",
    "eth-comp": "./node_modules/.bin/truffle compile ",
    "eth-test": "./node_modules/.bin/truffle test",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^9.4.7",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.4",
    "@types/react-router": "^4.0.29",
    "@types/react-router-redux": "^5.0.15",
    "@types/web3": "^1.0.3",
    "ganache-cli": "^6.1.6",
    "serve": "^9.4.2",
    "truffle": "^4.1.14",
    "typescript": "^3.0.1",
    "openzeppelin-solidity": "^1.12.0"
  }
}
